{
  "name": "@sfx/unifi-portal",
  "version": "1.0.0",
  "description": "A secure client portal for accessing UniFi network controllers, reports, and management tools.",
  "type": "module",
  "author": "S-FX.com Small Business Solutions. <shane@s-fx.com>",
  "license": "proprietary",
  "private": true,
  "scripts": {
    "vite:watch": "NODE_ENV=development vite",
    "vite:build": "NODE_ENV=production vite build",
    "vite:preview": "vite preview",
    "mjml:watch": "mjml --watch ./src/emails/templates/unifi-portal.mjml -o ./src/emails/templates/unifi-portal.html",
    "mjml:build": "mjml -r ./src/emails/templates/unifi-portal.mjml -o ./src/emails/templates/unifi-portal.html",
    "dev": "concurrently \"yarn run vite:watch\" \"yarn run mjml:watch\"",
    "prod": "concurrently \"yarn run vite:build\" \"yarn run mjml:build\""
  },
  "dependencies": {
