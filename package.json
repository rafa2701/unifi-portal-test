    "@vueuse/gesture": "^2.0.0",
    "@vueuse/motion": "^2.2.6",
    "axios": "^1.7.9",
    "chart.js": "^4.4.7",
    "concurrently": "^9.1.2",
    "dayjs": "^1.11.13",
    "floating-vue": "^5.2.2",
    "html2canvas": "^1.4.1",
    "html2pdf.js": "^0.10.2",
    "jspdf": "^2.5.2",
    "lodash": "^4.17.21",
    "lucide-vue-next": "^0.475.0",
    "mitt": "^3.0.1",
    "numeral": "^2.0.6",
